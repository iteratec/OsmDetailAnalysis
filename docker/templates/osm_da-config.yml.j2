                            osmUrl: {{ default .Env.OSMDA_INITIAL_OSM_URL3 "http://localhost:8080" }}
                            {{ end }}
                            {{ if .Env.OSMDA_INITIAL_API_KEY4 }}
                        -   key: {{ default .Env.OSMDA_INITIAL_API_KEY4 "myapikey" }}
                            osmUrl: {{ default .Env.OSMDA_INITIAL_OSM_URL4 "http://localhost:8080" }}
