server:
    port: 8081

grails:
    mongodb:
        host: '{{ default .Env.OSMDA_MONGODB_HOST "osm_mongodb" }}'
        port: 27017
        databaseName: OsmDetailAnalysis
    de:
        iteratec:
            osm:
                da:
                    cleanup:
                        daysToKeep: {{default .Env.OSMDA_DAYS_TO_KEEP 60}}
                    report.external:
                        graphiteServer:
                            serverAddress: {{ default .Env.GRAPHITE_CARBON_URL "null" }}
                            carbonPort: {{ default .Env.GRAPHITE_CARBON_PORT "null" }}
                    apiKeys:
                        list:
                        -   key: {{ default .Env.OSMDA_INITIAL_API_KEY "myapikey" }}
                            osmUrl: {{ default .Env.OSMDA_INITIAL_OSM_URL "http://localhost:8080" }}
                            {{ if .Env.OSMDA_INITIAL_API_KEY1 }}
                        -   key: {{ default .Env.OSMDA_INITIAL_API_KEY1 "myapikey" }}
                            osmUrl: {{ default .Env.OSMDA_INITIAL_OSM_URL1 "http://localhost:8080" }}
                            {{ end }}
                            {{ if .Env.OSMDA_INITIAL_API_KEY2 }}
